(
    inlet
    {
        type            patch;
        nFaces          5;
        startFace       130;
    }
    outlet
    {
        type            patch;
        nFaces          5;
        startFace       135;
    }
    bottom
    {
        type            symmetryPlane;
        inGroups        1(symmetryPlane);
        nFaces          5;
        startFace       140;
    }
    top
    {
        type            symmetryPlane;
        inGroups        1(symmetryPlane);
        nFaces          15;
        startFace       145;
    }
    obstacle
    {
        type            patch;
        nFaces          10;
        startFace       160;
    }
    defaultFaces
    {
        type            empty;
        inGroups        1(empty);
        nFaces          150;
        startFace       170;
    }
)

